{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.16/MicrosoftTeams.schema.json",
  "manifestVersion": "1.16",
  "version": "1.3.0",
  "id": "YOUR_APP_ID",
  "packageName": "com.databricks.genie",
  "developer": {
    "name": "Your Company Name",
    "websiteUrl": "https://example.com",
    "privacyUrl": "https://example.com/privacy",
    "termsOfUseUrl": "https://example.com/terms"
  },
  "icons": {
    "color": "icon-color.png",
    "outline": "icon-outline.png"
  },
  "name": {
    "short": "Databricks Genie",
    "full": "Databricks Genie Bot"
  },
  "description": {
    "short": "Query your data with natural language",
    "full": "A bot that connects to Databricks Genie to answer natural language questions about your data. Ask questions in plain English and get instant insights!"
  },
  "accentColor": "#5C6BC0",
  "bots": [
    {
      "botId": "YOUR_APP_ID",
      "scopes": ["personal", "team", "groupchat"],
      "supportsFiles": false,
      "isNotificationOnly": false
    }
  ],
  "permissions": ["identity", "messageTeamMembers"],
  "validDomains": [
    "YOUR_APP_NAME.azurewebsites.net",
    "token.botframework.com"
  ],
  "webApplicationInfo": {
    "id": "YOUR_APP_ID",
    "resource": "api://botid-YOUR_APP_ID"
  }
}
